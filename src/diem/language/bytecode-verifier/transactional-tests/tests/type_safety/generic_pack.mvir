//# publish
module 0x1.M {
    struct Foo<T> { x: T }

    foo() {
        let x: Self.Foo<u64>;
        // valid
        x = Foo<u64> { x : 42 };
        abort 0;
    }
}

//# publish
module 0x1.N {
    struct Foo<T1, T2: key> { x: T1, y: T2 }
    struct Bar has key { f: bool }

    foo() {
        let x: Self.Foo<u64, Self.Bar>;
        // valid
        x = Foo<u64, Self.Bar> { x : 42, y: Bar { f: false } };
        abort 0;
    }
}
