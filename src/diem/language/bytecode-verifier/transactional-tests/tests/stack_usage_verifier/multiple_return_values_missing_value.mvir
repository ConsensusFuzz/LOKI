//# publish
module 0x1.Test {
    struct T { b: bool }

    public new(): Self.T {
        return T { b: true };
    }

    public test(i: &u64, x: Self.T): u64 * Self.T * bool {
        // too few values on stack for return, negative stack size
        return *move(i), move(x);
    }

    public destroy(x: Self.T) {
        let b: bool;
        T { b } = move(x);
        return;
    }
}
