//# publish
module 0x42.Tester {
    struct T has drop {v: u64}

    public new(v: u64): Self.T  {
        return T{v: move(v)};
    }

    public replace(t: &mut Self.T) {
        let t_v: &mut u64;
        let new_t: Self.T;

        t_v = &mut copy(t).T::v;
        new_t = Self.new(1);
        // cannot write to t, still borrowed
        *move(t) = move(new_t);

        assert(*move(t_v) == 1, 42);
        return;
    }
}
