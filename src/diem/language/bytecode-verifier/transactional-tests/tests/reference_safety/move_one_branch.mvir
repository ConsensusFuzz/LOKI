module 0x1.M {
    t0(cond: bool) {
        let x: u64;
        let x_ref: &mut u64;
        x = 0;
        x_ref = &mut x;
        if (move(cond)) {
            *move(x_ref) = 1;
        }
        // safe to assign, as x is no longer borrowed
        // even though the reference was only moved in one branch
        x = 1;
        return;
    }

    t1(cond: bool) {
        let x: u64;
