//# publish
module 0x1.Test {
    public t(): u64 {
        let x: u64;
        if (true) {
            return 100;
        } else {
            x = 0;
        }
        return copy(x);
    }
}

//# run
import 0x1.Test;

main() {
    let x: u64;
    x = Test.t();
    assert(copy(x) == 100, 42);
    return;
}
