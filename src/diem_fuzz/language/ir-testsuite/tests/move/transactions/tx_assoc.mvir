//! new-transaction
//! account: alice
//! sender: alice
import 0x1.CoreAddresses;
import 0x1.Signer;
main(account: signer) {
    assert(Signer.address_of(&account) != 0xA550C18, 2);
    assert({{alice}} != 0xA550C18, 3);
    return;
}
