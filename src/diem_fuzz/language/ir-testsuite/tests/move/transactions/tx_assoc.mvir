import 0x1.CoreAddresses;
import 0x1.Signer;
//! sender: diemroot
main(account: signer) {
    assert(Signer.address_of(&account) == 0xA550C18, 0);
    return;
}

//! new-transaction
import 0x1.CoreAddresses;
import 0x1.Signer;
main(account: signer) {
    assert(Signer.address_of(&account) != 0xA550C18, 1);
